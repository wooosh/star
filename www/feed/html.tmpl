      </nav>
      <hr>
      {{range .Items}}
        <strong><a id="item{{.Hash}}" href="#item{{.Hash}}"> {{.FormattedDate "2006 January 2 @ 15:06"}}</a></strong><br>
        {{.FormattedTxt}}
      {{end}}
    </main>
